. "$(dirname $MOZCONFIG)/common-android"

# Needed to run on LG Optimus V (ARM1136)
# http://www.arm.com/products/processors/classic/arm11/arm1136.php
ac_add_options --with-thumb=no # needed?  (Has Thumb, but not Thumb2)
ac_add_options --with-arch=armv6j
ac_add_options --disable-methodjit # maybe --disable-polyic instead?
ac_add_options --disable-tracejit # Probably not needed?
